<h1>My Images</h1>

<%= link_to 'New Image', new_image_path %>

<table>
  <tr>
    <th>Url</th>
    <th>Tags</th>
    <th></th>
  </tr>

  <% @images.each do |image| %>
    <tr>
      <td><%= image.url %></td>
      <td><%= image.tag_list %></td>
      <td><%= image_tag(image.url, class: 'image-view') %></td>
    </tr>
  <% end %>
</table>
